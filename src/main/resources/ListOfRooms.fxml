<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ListOfRoomsController">
    <children>
        <TableView fx:id="tableViewRooms" layoutX="10" layoutY="10" prefHeight="353.0" prefWidth="580">
            <columns>
                <TableColumn fx:id="columnRoomNumber" prefWidth="150" text="Room Number" />
                <TableColumn fx:id="columnType" prefWidth="150" text="Type" />
                <TableColumn fx:id="columnPrice" prefWidth="150" text="Price" />
            <TableColumn fx:id="columnStatus" prefWidth="150" text="Status" />
            </columns>
        </TableView>
      <Button fx:id="back" layoutX="543.0" layoutY="364.0" mnemonicParsing="false" text="Back">
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
      </Button>
      <Button fx:id="bt_book" layoutX="153.0" layoutY="363.0" mnemonicParsing="false" text="book" />
      <TextField fx:id="tf_roomnumber" layoutX="49.0" layoutY="363.0" prefHeight="26.0" prefWidth="94.0" promptText="room number" />
    </children>
</AnchorPane>
